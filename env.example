# Environment variables for FHE Credit Default Risk Prediction Application

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# Streamlit Configuration
STREAMLIT_HOST=0.0.0.0
STREAMLIT_PORT=8501

# Model Configuration
MODEL_PATH=models/credit_model.pkl
